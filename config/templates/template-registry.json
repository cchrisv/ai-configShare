{
  "version": "1.0.0",
  "templates": {
    "field-user-story-description": {
      "file": "field-user-story-description.html",
      "phase": "grooming",
      "work_item_types": ["User Story"],
      "ado_field": "System.Description",
      "description": "User Story description with summary, user story format, goals, assumptions, unknowns, constraints, and out-of-scope",
      "variables": {
        "summary_text": { "type": "text", "required": true, "description": "Business summary of the user story" },
        "persona": { "type": "text", "required": true, "description": "User persona (As a...)" },
        "action": { "type": "text", "required": true, "description": "Desired action (I want to...)" },
        "business_value": { "type": "text", "required": true, "description": "Business value (so that...)" },
        "goals": { "type": "list", "required": true, "description": "Goals and business value items", "min_items": 1, "token_prefix": "goal" },
        "assumptions": {
          "type": "table", "required": true, "description": "Assumptions to validate during solutioning", "token_prefix": "assumption",
          "columns": [
            { "key": "id", "header": "ID", "required": true },
            { "key": "statement", "header": "Statement", "required": true },
            { "key": "confidence", "header": "Confidence", "required": true },
            { "key": "status", "header": "Status", "required": true },
            { "key": "falsify", "header": "How to Falsify", "required": true }
          ]
        },
        "unknowns": {
          "type": "table", "required": true, "description": "Known unknowns requiring clarification", "token_prefix": "unknown",
          "columns": [
            { "key": "id", "header": "ID", "required": true },
            { "key": "question", "header": "Question", "required": true },
            { "key": "impact", "header": "Impact", "required": true },
            { "key": "who", "header": "Who Can Clarify", "required": true }
          ]
        },
        "constraints": { "type": "list", "required": true, "description": "Technical and business constraints", "min_items": 1, "token_prefix": "constraint" },
        "out_of_scope": { "type": "list", "required": true, "description": "Items explicitly out of scope", "min_items": 1 }
      },
      "sections": [
        { "id": "summary", "name": "Summary", "required": true, "gradient_signature": "#1565c0" },
        { "id": "user_story", "name": "User Story", "required": true, "gradient_signature": "#7b1fa2" },
        { "id": "goals", "name": "Goals & Business Value", "required": true, "gradient_signature": "#2e7d32" },
        { "id": "assumptions", "name": "Assumptions", "required": true, "gradient_signature": "#e65100" },
        { "id": "unknowns", "name": "Known Unknowns", "required": true, "gradient_signature": "#c62828" },
        { "id": "constraints", "name": "Constraints", "required": true, "gradient_signature": "#303f9f" },
        { "id": "out_of_scope", "name": "Out of Scope", "required": true, "gradient_signature": "#ad1457" }
      ],
      "has_repeatable_regions": true
    },

    "field-user-story-acceptance-criteria": {
      "file": "field-user-story-acceptance-criteria.html",
      "phase": "grooming",
      "work_item_types": ["User Story"],
      "ado_field": "Microsoft.VSTS.Common.AcceptanceCriteria",
      "description": "User Story acceptance criteria in Given/When/Then scenario card format",
      "variables": {
        "scenarios": {
          "type": "repeatable_block", "required": true, "description": "GWT scenario cards (min 3)", "token_prefix": "scenario",
          "min_items": 3,
          "block_variables": {
            "title": { "type": "text", "required": true, "description": "Scenario title" },
            "given": { "type": "text", "required": true, "description": "Given precondition" },
            "when": { "type": "text", "required": true, "description": "When action" },
            "then": { "type": "text", "required": true, "description": "Then expected result" }
          }
        }
      },
      "sections": [
        { "id": "header", "name": "Acceptance Criteria", "required": true, "gradient_signature": "#1565c0" }
      ],
      "has_repeatable_regions": true
    },

    "field-bug-description": {
